                        "sql": "SELECT\n\t\"OperationalContract\".\"ID\" AS \"ID\",\n\t\"OperationalContract\".\"ContractWorkspace_ID\" AS \"ContractWorkspace\",\n    COALESCE(\"CostCenter\".\"ID\", \"CostCenterZeros\".\"ID\") AS \"CostCenter\"\nFROM \"o_custom_OperationalContract\" AS \"OperationalContract\"\nLEFT JOIN \"o_custom_ContractWorkspace\" AS \"ContractWorkspace\" ON 1=1\n    AND \"OperationalContract\".\"ContractWorkspace_ID\" = \"ContractWorkspace\".\"ID\"\nLEFT JOIN \"o_custom_CostCenter\" AS \"CostCenter\" ON 1=1\n    AND \"ContractWorkspace\".\"CostCenter\" = \"CostCenter\".\"CostCenter\"\nLEFT JOIN \"o_custom_CostCenter\" AS \"CostCenterZeros\" ON 1=1\n    AND RIGHT('000000000' || \"ContractWorkspace\".\"CostCenter\",10) = \"CostCenterZeros\".\"CostCenter\""
